{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "moduleNameMapper": {
      "^@src/(.*)$": "<rootDir>../src/$1"
    },
  "modulePaths":["<rootDir>"],
  "testRegex": "spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "coverageDirectory": "./coverage",
  "testEnvironment": "node"
}
